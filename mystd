namespace mystd {

	template <typename T1, typename T2>
	class node {
	private:
	public:
		T1 key;
		T2 value;
		node* right;
		node* left;
		node(T1 _key, T2 _value) {
			key = _key;
			value = _value;
			right = NULL;
			left = NULL;
		}; // Constructor
		T1 get_key() {
			return key;
		}
		T2 get_value() {
			return value;
		}

	};
};
